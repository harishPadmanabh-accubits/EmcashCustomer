<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SemiCircleArcProgressBar">
        <attr name="progressPlaceHolderColor" format="color"/>
        <attr name="progressBarColor" format="color"/>
        <attr name="progressPlaceHolderWidth" format="integer"/>
        <attr name="progressBarWidth" format="integer"/>
        <attr name="percent" format="integer"/>
    </declare-styleable>

    <declare-styleable name="OnboardingNextButton">
        <attr name="buttonText" format="string"/>
    </declare-styleable>

    <declare-styleable name="CoinProfileImageView">
        <attr name="coin_size" format="enum">
            <enum name="semi_large" value="0"/>
            <enum name="large" value="1"/>
            <enum name="small" value="2"/>
            <enum name="tiny" value="3"/>
            <enum name="normal" value="4"/>
            <enum name="medium" value="5"/>
        </attr>
    </declare-styleable>
</resources>